{
    "name": "DeepRTE",
    "dockerComposeFile": "compose.yaml",
    "service": "deeprte",
    "workspaceFolder": "/deeprte",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-python-envs@prerelease",
                "ms-toolsai.tensorboard",
                "ms-toolsai.jupyter",
                "astral-sh.ty@prerelease",
                "charliermarsh.ruff",
                "redhat.vscode-yaml",
                "tamasfe.even-better-toml"
            ]
        }
    },
    "postCreateCommand": {
        "fix-volume-permissions": "sudo chown deeprte data .venv /home/deeprte/.cache  ",
        "sync-environment": "uv sync --all-extras"
    },
    "remoteUser": "deeprte"
}
